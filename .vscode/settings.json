{
    "Lua.runtime.version": "LuaJIT",
    "Lua.workspace.library": [
        "${3rd}/love2d/library"
    ],
    "Lua.workspace.checkThirdParty": false,
    "Lua.diagnostics.disable": [
        "unused-local"
    ],
    "Lua.diagnostics.globals": [
        "love",
        "configure",
        "north_key",
        "east_key",
        "south_key",
        "west_key",
        "states",
        "state",
        "menu",
        "up_key",
        "right_key",
        "down_key",
        "left_key",
        "rooms",
        "Room",
        "enter_key",
        "game",
        "player"
    ],
    "editor.formatOnSave": true,
    "Lua.completion.autoRequire": true,
    "Lua.completion.autoImport": true,
    "[lua]": {
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        },
        "editor.tabCompletion": "on",
        "editor.quickSuggestionsDelay": 1
    },
    "lua.targetVersion": "5.3",
    "restructuredtext.confPath": "${workspaceFolder}/libs/hump/docs"
}