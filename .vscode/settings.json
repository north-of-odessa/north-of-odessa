{
  "Lua.runtime.version": "LuaJIT",
  "Lua.workspace.library": ["${3rd}/love2d/library"],

  "Lua.diagnostics.globals": [
    "love",
    "configure",
    "north_key",
    "east_key",
    "south_key",
    "west_key",
    "states",
    "state",
    "menu",
    "up_key",
    "right_key",
    "down_key",
    "left_key",
    "Rooms",
    "Rooms",
    "enter_key",
    "game",
    "player",
    "common",
    "class_commons",
    "_",
    "rgba",
    "hex",
    "hexVal",
    "char"
  ],
  "editor.formatOnSave": true,
  "Lua.completion.autoRequire": true,
  "[lua]": {
    "editor.formatOnSaveMode": "file",
    "editor.defaultFormatter": "tangzx.emmylua",
    "editor.formatOnSave": true,
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true,
      "paths": true
    },
    "editor.tabCompletion": "on",
    "editor.quickSuggestionsDelay": 1
  },
  "lua.targetVersion": "5.3",
  "Lua.diagnostics.disable": [
  ],
  "Lua.workspace.checkThirdParty": false
}
